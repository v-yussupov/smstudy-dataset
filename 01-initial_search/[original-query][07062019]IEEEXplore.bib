month={Oct},}
@INPROCEEDINGS{6095981,
author={F. {Wieland} and M. {Santos} and W. {Krueger} and V. E. {Houston}},
booktitle={2011 IEEE/AIAA 30th Digital Avionics Systems Conference},
title={Performance of Airborne Precision Spacing under realistic weather conditions},
year={2011},
volume={},
