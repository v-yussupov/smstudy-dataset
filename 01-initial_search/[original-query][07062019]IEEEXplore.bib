@INPROCEEDINGS{6382358,
author={E. {Kim}},
booktitle={2012 IEEE/AIAA 31st Digital Avionics Systems Conference (DASC)},
title={Hybrid APNT architecture using DME/DME and multilateration},
year={2012},
volume={},
