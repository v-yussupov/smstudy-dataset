@INPROCEEDINGS{6382337,
author={T. {Kozon} and A. {Farrahi} and S. {Verma} and H. {Tang} and D. {Ballinger}},
booktitle={2012 IEEE/AIAA 31st Digital Avionics Systems Conference (DASC)},
title={Phase-2 evaluation of a Tactical conflict detection tool in the Terminal area},
