@INPROCEEDINGS{7311427,
author={C. A. {Wargo} and G. {Hunter} and K. {Leiden} and J. {Glaneuski} and B. {Van Acker} and K. {Hatton}},
booktitle={2015 IEEE/AIAA 34th Digital Avionics Systems Conference (DASC)},
title={New entrants (RPA/space vehicles) operational impacts upon NAS ATM and ATC},
year={2015},
volume={},
