@INPROCEEDINGS{5935343,
author={S. {Stalnaker} and C. E. {Morgan} and K. {Rammelsberg} and R. {Sgorcea} and A. {Shaw} and A. M. {Staley}},
booktitle={2011 Integrated Communications, Navigation, and Surveillance Conference Proceedings},
title={Functional allocation of initial surface decision support tools to tower and traffic flow management systems},
