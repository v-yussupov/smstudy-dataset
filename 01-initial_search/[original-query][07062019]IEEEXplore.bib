@INPROCEEDINGS{6382321,
author={J. {Rios} and R. {Jehlen} and Z. {Zhu}},
booktitle={2012 IEEE/AIAA 31st Digital Avionics Systems Conference (DASC)},
title={A spatial database for reroute planning},
year={2012},
