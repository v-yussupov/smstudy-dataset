@INPROCEEDINGS{6979467,
author={E. {Letsu-Dake} and D. {Pepitone} and J. {Ball} and R. {Burgin}},
booktitle={2014 IEEE/AIAA 33rd Digital Avionics Systems Conference (DASC)},
title={Recommendations for managing complexity in electronic chart information displays},
