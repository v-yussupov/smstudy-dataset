@INPROCEEDINGS{8384858,
author={R. L. {Stroup}},
booktitle={2018 Integrated Communications, Navigation, Surveillance Conference (ICNS)},
title={Aviation strategic outlook areas driving the NAS CNS-ATM system of systems},
