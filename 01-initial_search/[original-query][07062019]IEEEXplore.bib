@INPROCEEDINGS{7311332,
author={S. {Torres} and J. {Dehn} and E. {McKay} and M. M. {Paglione} and B. S. {Schnitzer}},
booktitle={2015 IEEE/AIAA 34th Digital Avionics Systems Conference (DASC)},
title={En-Route Automation Modernization (ERAM) trajectory model evolution to support trajectory-based operations (TBO)},
year={2015},
volume={},
number={},
pages={1A6-1-1A6-15},
abstract={The En Route Automation Modernization (ERAM) system is the Federal Aviation Administration's (FAA) automation platform used to manage air traffic at 20 en route centers in the US. This paper describes the trajectory model in the currently operational ERAM system and presents an approach to enhance ERAM's aircraft performance model with a high fidelity kinetic capability that can provide greater consistency with the guidance trajectory generated by Flight Management Systems (FMS). ERAM has a parametric trajectory predictor that uses static aircraft data. A physics-based kinetic trajectory predictor, on the other hand, can exploit flight-specific aircraft intent that will become increasingly available as the Operational Improvements in the Next Generation Air Transportation (NextGen) system are deployed. Additionally, a kinetic model will provide increased interoperability between air traffic management systems. A kinetic aircraft performance model for ERAM has been prototyped and evaluated. When flight-specific intent information is available or can be derived, the accuracy of predicting the top of descent (TOD) is significantly improved with a kinetic model. An ERAM test run with the kinetic prototype based on recorded traffic of 50 flights executing idle thrust descents shows that, relative to the baseline ERAM legacy system, the number of trajectories with along-track TOD prediction errors less than 5 nautical miles increases from 9% to 63%. Considering the incremental nature of the NextGen implementation, a low risk approach to evolving ERAM is to first move to a hybrid kinetic-parametric trajectory model, where current parametric modeling is retained and kinetic modeling is performed when flight-specific intent is available.},
keywords={air traffic control;aircraft control;hybrid kinetic-parametric trajectory model;ERAM legacy system;TOD;top of descent prediction;kinetic aircraft performance model;air traffic management systems;NextGen system;next generation air transportation system;flight-specific aircraft;physics-based kinetic trajectory predictor;static aircraft data;FMS;flight management systems;ERAM aircraft performance model;FAA;Federal Aviation Administration automation platform;TBO;trajectory-based operations;en-route automation modernization trajectory model;Trajectory;Atmospheric modeling;Aircraft;Adaptation models;Kinetic theory;Aircraft manufacture;Automation},
doi={10.1109/DASC.2015.7311332},
ISSN={2155-7195},
month={Sep.},}
@INPROCEEDINGS{7311339,
author={M. E. {Miller} and W. D. {Hall}},
booktitle={2015 IEEE/AIAA 34th Digital Avionics Systems Conference (DASC)},
