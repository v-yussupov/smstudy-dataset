month={Oct},}
@INPROCEEDINGS{6979499,
author={H. {Zhang} and S. {Wei} and W. {Yu} and E. {Blasch} and G. {Chen} and D. {Shen} and K. {Pham}},
booktitle={2014 IEEE/AIAA 33rd Digital Avionics Systems Conference (DASC)},
title={Scheduling methods for unmanned aerial vehicle based delivery systems},
year={2014},
volume={},
