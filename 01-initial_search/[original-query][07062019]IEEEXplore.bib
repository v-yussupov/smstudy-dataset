@INPROCEEDINGS{8011929,
author={C. {Snipes} and J. {DiFelici} and C. {Wargo}},
booktitle={2017 Integrated Communications, Navigation and Surveillance Conference (ICNS)},
title={UAS and wireless spectrum: Modeling data link demand},
year={2017},
