@INPROCEEDINGS{5935290,
author={V. {Mehta} and M. E. {Miller} and T. {Reynolds} and M. {Ishutkina} and R. {Jordan} and R. {Seater} and W. {Moser}},
booktitle={2011 Integrated Communications, Navigation, and Surveillance Conference Proceedings},
title={Decision support tools for the tower flight data manager system},
