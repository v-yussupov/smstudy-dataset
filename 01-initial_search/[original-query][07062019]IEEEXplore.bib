@INPROCEEDINGS{7118970,
author={K. {Ueunten} and C. {Lum} and A. {Creigh} and K. {Tsujita}},
booktitle={2015 IEEE Aerospace Conference},
title={Conservative algorithms for automated collision awareness for multiple Unmanned Aerial Systems},
year={2015},
volume={},
