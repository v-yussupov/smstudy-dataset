@INPROCEEDINGS{6215139,
author={K. K. {Hughes} and C. Y. {Wu} and J. L. {Lamp} and A. G. {Coleman} and P. {Adili} and S. K. {Dhanoya} and K. P. {White}},
booktitle={2012 IEEE Systems and Information Engineering Design Symposium},
title={Optimizing airport arrival operations: A systems engineering analysis of Washington Dulles International Airport},
year={2012},
volume={},
