@INPROCEEDINGS{6382401,
author={C. E. {Morgan} and R. M. {Stanley}},
booktitle={2012 IEEE/AIAA 31st Digital Avionics Systems Conference (DASC)},
title={Exploring an approach to capability maturity assessment: Towards an evidence-based framework},
year={2012},
volume={},
