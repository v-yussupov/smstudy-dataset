@INPROCEEDINGS{6095976,
author={A. {Khedkar} and V. {Kumar}},
booktitle={2011 IEEE/AIAA 30th Digital Avionics Systems Conference},
title={Self synchronization based air traffic control and collision avoidance system},
year={2011},
