doi={10.1109/DASC.2013.6712530},
ISSN={2155-7195},
month={Oct},}
@INPROCEEDINGS{6712531,
author={E. T. {Evans} and S. D. {Young} and T. S. {Daniels} and R. R. {Myer}},
