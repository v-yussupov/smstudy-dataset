@INPROCEEDINGS{5935283,
author={I. {Gheorghisor} and Y. {Hoh} and A. {Leu}},
booktitle={2011 Integrated Communications, Navigation, and Surveillance Conference Proceedings},
title={Compatibility of airport wireless broadband networks with satellite links in the 50915150 MHz band},
year={2011},
volume={},
