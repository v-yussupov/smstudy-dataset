doi={10.1109/DASC.2016.7778100},
ISSN={2155-7209},
month={Sep.},}
@INPROCEEDINGS{7778104,
author={M. {Xue} and S. {Zelinski}},
booktitle={2016 IEEE/AIAA 35th Digital Avionics Systems Conference (DASC)},
title={Route optimization for offloading congested meter fixes},
year={2016},
