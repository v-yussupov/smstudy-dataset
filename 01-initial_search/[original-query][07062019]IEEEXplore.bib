@INPROCEEDINGS{6712503,
author={J. {Thipphavong} and J. {Jung} and H. {Swenson} and L. {Martin} and M. {Lin} and J. {Nguyen}},
booktitle={2013 IEEE/AIAA 32nd Digital Avionics Systems Conference (DASC)},
title={Evaluation of the Terminal Sequencing and Spacing system for Performance-Based Navigation arrivals},
year={2013},
