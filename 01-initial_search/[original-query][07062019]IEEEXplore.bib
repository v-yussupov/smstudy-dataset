month={Sep.},}
@INPROCEEDINGS{7311387,
author={B. {Matthews} and D. {Nielsen} and J. {Schade} and K. {Chan} and M. {Kiniry}},
booktitle={2015 IEEE/AIAA 34th Digital Avionics Systems Conference (DASC)},
title={Comparative study of metroplex airspace and procedures using machine learning to discover flight track anomalies},
year={2015},
