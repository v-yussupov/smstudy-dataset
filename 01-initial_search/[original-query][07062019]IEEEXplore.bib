month={Oct},}
@INPROCEEDINGS{6979512,
author={J. {DeArmon} and A. {Mahashabde} and B. {Hogan}},
booktitle={2014 IEEE/AIAA 33rd Digital Avionics Systems Conference (DASC)},
title={Departure flow efficiency and the identification of causes for inefficiencies},
year={2014},
