@INPROCEEDINGS{7486321,
author={M. {Moallemi} and C. A. {Castro-Pea} and M. {Towhidnejad} and B. {Abraham}},
booktitle={2016 Integrated Communications Navigation and Surveillance (ICNS)},
title={Information security in the Aircraft Access to System Wide Information Management infrastructure},
year={2016},
volume={},
