doi={10.1109/DASC.2014.6979509},
ISSN={2155-7195},
month={Oct},}
@INPROCEEDINGS{6979510,
author={Y. {Wang}},
booktitle={2014 IEEE/AIAA 33rd Digital Avionics Systems Conference (DASC)},
title={Analysis and prediction of weather impacted ground stop operations},
