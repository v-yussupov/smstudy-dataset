@INPROCEEDINGS{6979469,
author={B. {Matthews} and D. {Nielsen} and J. {Schade} and K. {Chan} and M. {Kiniry}},
booktitle={2014 IEEE/AIAA 33rd Digital Avionics Systems Conference (DASC)},
title={Automated discovery of flight track anomalies},
year={2014},
volume={},
