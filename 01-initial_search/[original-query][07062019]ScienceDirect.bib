booktitle = "Practical Airport Operations, Safety, and Emergency Management",
publisher = "Butterworth-Heinemann",
pages = "137 - 189",
year = "2016",
isbn = "978-0-12-800515-6",
doi = "https://doi.org/10.1016/B978-0-12-800515-6.00006-8",
url = "http://www.sciencedirect.com/science/article/pii/B9780128005156000068",
author = "Jeffrey C. Price and Jeffrey S. Forrest",
keywords = "Airside Self-Inspection, Airport Operating Certificate (AOC), Airport Certification Manual (ACM), National Incident Management System (NIMS), Airport Compliance Safety Inspectors (ACSI), , Compliance Safety Inspector",
abstract = "Chapter 6—Airside Operations: Safety Self-Inspections examines required tasks, processes, and associated regulatory concerns for conducting airside safety inspections. Airside Operations conducts daily and continuous inspections to detect safety-related issues and conditions not in compliance with Title 14 CFR Part 139, Airport Certification. General aviation (GA) airports are not required to comply with Part 139. Nevertheless, GA operations typically conducts similar Part 139 inspections as part of complying with Grant Assurance 19, requiring the airport to be operated in a safe condition. Airports under Part 139 have an Airport Operating Certificate (AOC) from the FAA. Prior to issuance of an AOC, airport operators must draft an Airport Certification Manual (ACM), outlining how the airport will meet regulatory standards under Part 139. Once certificated, the airport operator is responsible for the daily and ongoing compliance with the ACM, with periodic inspections conducted by the FAA."
}
@incollection{PRICE201619,
title = "Chapter 2 - Operating Airports as Complex and Regulated Global Resources",
editor = "Jeffrey C. Price and Jeffrey S. Forrest",
booktitle = "Practical Airport Operations, Safety, and Emergency Management",
publisher = "Butterworth-Heinemann",
pages = "19 - 38",
year = "2016",
isbn = "978-0-12-800515-6",
doi = "https://doi.org/10.1016/B978-0-12-800515-6.00002-0",
url = "http://www.sciencedirect.com/science/article/pii/B9780128005156000020",
author = "Jeffrey C. Price and Jeffrey S. Forrest",
keywords = "Airport Management, Airport Legislation, Airport Policy, Airport Administration, Airport Sponsor, State Aeronautics, Airport Communications Center, Airport Director, Airport Improvement Program",
abstract = "Early US aviation was primarily unregulated and comprised mostly transient pilots landing in farms or other areas enabling takeoff and landing operations. Chapter 2—Operating Airports as Complex and Regulated Global Resources provides an overview of critical factors generating the need for and development of airports and the overall role of airports in society. Included are key legislation, policies, and professional and government organizations influencing how airports were developed and are now operated. Key management structure of Airport Operations is outlined within the context of airports as a service to many stakeholders. The chapter explains how airports that receive federal aid are held to certain obligations by the Federal Aviation Administration (FAA) and, to a certain extent, the Transportation Security Administration (TSA). Sample airport organizational structures are provided, along with explanations of important regulatory agencies and trade organizations."
}
@incollection{PRICE201639,
title = "Chapter 3 - Operations and the Airport Environment",
editor = "Jeffrey C. Price and Jeffrey S. Forrest",
booktitle = "Practical Airport Operations, Safety, and Emergency Management",
publisher = "Butterworth-Heinemann",
pages = "39 - 65",
year = "2016",
isbn = "978-0-12-800515-6",
doi = "https://doi.org/10.1016/B978-0-12-800515-6.00003-2",
url = "http://www.sciencedirect.com/science/article/pii/B9780128005156000032",
author = "Jeffrey C. Price and Jeffrey S. Forrest",
keywords = "FAA Airport Grant Assurances, Airport Compliance Manual, Minimum Standards for Commercial Aeronautical Activities, Airport Tenants, Airport Operational Functions",
abstract = "Chapter 3—Operations and the Airport Environment describes how Airports Operations (Ops) and the airport environment are affected by a variety of regulations, legislative acts, judicial decisions, and grant assurances. When public-use airports accept grant funding from the Federal Aviation Administration (FAA), the airport is obligated to sustain a safe operating environment by complying with FAA-defined operating requirements (e.g., pavement maintenance, hazard reporting, and debris removal). Rules and policies are drafted by the airport to ensure a safe and secure operating environment, and Ops is responsible for their enforcement. In this responsibility, airport operational personnel act as ambassadors to the airport management team by maintaining relationships and solving problems for airport tenants, government agencies, and the public. The core functions of Ops are inspecting the airfield environment, responding to emergencies, and monitoring weather conditions in order to implement proper contingency plans."
}
@incollection{PRICE2016557,
title = "Chapter 13 - Emerging Issues in Airport Operations, Safety, and Emergency Management",
editor = "Jeffrey C. Price and Jeffrey S. Forrest",
booktitle = "Practical Airport Operations, Safety, and Emergency Management",
publisher = "Butterworth-Heinemann",
pages = "557 - 590",
year = "2016",
isbn = "978-0-12-800515-6",
doi = "https://doi.org/10.1016/B978-0-12-800515-6.00013-5",
url = "http://www.sciencedirect.com/science/article/pii/B9780128005156000135",
author = "Jeffrey C. Price and Jeffrey S. Forrest",
keywords = "Unmanned Aerial Vehicles (UAVs), Unmanned Aerial Systems (UASs), Remotely Piloted Vehicles (RPVs), Vertical Takeoff and Landing Aircraft (VTOL), Commercial Spaceport Operations, Spacecraft Operations, Hypersonic Aircraft Operations, Crisis Leadership, Operant Conditioning, Certificate of Waiver or Authorization (COA), UAVport, Droneport, Spaceports",
abstract = "In Emerging Issues in Airport Operations, Safety, and Emergency Management, the future of Airport Operations, safety, and emergency management is shown to evolve in ways that will support new advents, such as Unmanned Aerial Vehicles (UAVs), commercialized aerospace systems, and innovative methods in airport emergency/incident management. UAVs have deployed throughout the US airspace system at rates faster than government can effectively regulate. Challenges include small UAVs operating near airports, by individuals who perhaps do not understand or abide by the regulations on such operations; and large UAVs that can present greater operational and safety challenges to airports and aircraft. Alternatively, UAVs also represent a significant potential source of revenue for airports. In addition to the adoption of UAVs, advancing technology and the cessation of the US Space Shuttle have opened new opportunities for the private commercial space industry operating at commercial airports. Several US airports have been designated or are seeking status as FAA-certified spaceports (see included case study of Front Range Airport, Denver, Colorado). There have been several flight tests of spacecraft designed to eventually take space tourists on suborbital flights. Commercial launch missions have been successful at servicing the International Space Station. Parallel to these innovations, it is vital that operational personnel and emergency Incident Commanders improve their understanding of psychological dynamics during high-risk/high-stress operations. Continuous improvement and delivery of training and exercises for operational personnel and emergency responders are now an absolute requirement in all areas of the aviation and aerospace industries."
}
@article{PRYMACZOK2012176,
title = "Survival, growth, and physiological responses of advanced juvenile freshwater crayfish (Cherax quadricarinatus), reared at low temperature and high salinities",
journal = "Aquaculture",
volume = "334-337",
pages = "176 - 181",
year = "2012",
issn = "0044-8486",
doi = "https://doi.org/10.1016/j.aquaculture.2011.12.032",
url = "http://www.sciencedirect.com/science/article/pii/S0044848611010143",
author = "Natalia Cecilia Prymaczok and Anouk Chaulet and Daniel Alberto Medesani and Enrique Marcelo Rodríguez",
keywords = "Temperature, Salinity, Stress, Metabolites, ",
abstract = "The effects of stress caused by low temperature, high salinities, and a combination of both low temperatures and high salinities were evaluated in advanced (~5g) juvenile freshwater crayfish (Cherax quadricarinatus). Ten animals were weighed and assigned to each of the following combination of two temperature and three salinity treatments: 27°C (optimum for this species) or 20°C and 0, 5, and 10g/L salt concentrations. After 30days in each treatment, oxygen consumption and weight were recorded, together with hemolymph levels of glucose, sodium, potassium, and free amino acids (FAA). Glycogen level was determined in hepatopancreas and abdominal muscle, while FAA levels were measured in abdominal muscle and hemolymph. A significant decrease of weight gain was seen at the combination of 20°C and 10g/L salinity. A marked hyperglycemia was seen at the lower temperature, at any salinity tested. At the same temperature, there was a concomitant decrease of glycogen, in both hepatopancreas and muscle. Both sodium and potassium hemolymphatic levels significantly increased with increasing salinity, but only at 20°C. No changes were seen in hemolymphatic FAA levels, but they increased in abdominal muscle at higher salinities, in correspondence with the sodium hemolymphatic increase, in order to regulate cellular osmolarity. Although advanced juveniles of C. quadricarinatus did not exhibit reduced survival or growth at 20°C or 10g/L salinity, the combination of relatively low temperature and high salinity significantly reduces growth, suggesting that the combination of these two factors is a stressful condition for these crayfish juveniles."
}
@article{PUSHKAR2019652,
title = "Modeling the substitution of natural materials with industrial byproducts in green roofs using life cycle assessments",
journal = "Journal of Cleaner Production",
volume = "227",
pages = "652 - 661",
year = "2019",
issn = "0959-6526",
doi = "https://doi.org/10.1016/j.jclepro.2019.04.237",
url = "http://www.sciencedirect.com/science/article/pii/S0959652619313289",
author = "Svetlana Pushkar",
keywords = "Byproduct, Natural material replacement, Allocation approach, LCA, Environmental impact, ANOVA",
abstract = "This study conducted life cycle assessments (LCAs) on four types of extensive green roofs and involved replacing natural perlite with the byproducts coal bottom ash (CBA) and fly ash-based aggregates (FAAs) in both the substrate and drainage layers of green roofs. The following combinations were investigated: perlite in both the substrate and drainage layers (PerliteSUB-PerliteDRIN), perlite in the substrate layer and FAAs in the drainage layer (PerliteSUB-FAADRIN), CBA in the substrate layer and perlite in the drainage layer (CBASUB-PerliteDRIN), and CBA in the substrate layer and FAAs in the drainage layer (CBASUB-FAADRIN). Two types of allocation approaches including mass allocation and system expansion were applied to the byproducts. The LCAs encompassed the production stage and the end-of-life stage. A two-stage analysis of variance (ANOVA) was used to analyze the ReCiPe results and the following was confirmed: (i) the analysis identified the best environmentally friendly roof alternative, (ii) it was observed that the results from the simplification of the LCA for the roof (omitting the end-of-life stage) depended on the type of evaluation approach used in the LCA, and (iii) the more comprehensive performance of the LCAs of green roofs was associated with a combination of the ReCiPe and ANOVA methodologies."
}
