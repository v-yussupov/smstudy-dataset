keywords = "Chen–Fliess functional expansion, Output feedback control system, Faà di Bruno Hopf algebra, Connected graded Hopf algebra, Zimmermann's forest formula",
abstract = "In this work a combinatorial description is provided of a Faà di Bruno type Hopf algebra which naturally appears in the context of Fliess operators in nonlinear feedback control theory. It is a connected graded commutative and non-cocommutative Hopf algebra defined on rooted circle trees. A cancellation free forest formula for its antipode is given."
}
@article{DUMMEE2012204,
title = "Bioaccumulation of heavy metals in water, sediments, aquatic plant and histopathological effects on the golden apple snail in Beung Boraphet reservoir, Thailand",
