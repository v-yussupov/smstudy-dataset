url = "http://www.sciencedirect.com/science/article/pii/S0001870819302221",
author = "Alessandra Frabetti and Ivan P. Shestakov",
keywords = "Loops, Algebraic groups, Non-associative algebras, Cogroups, Coloops, Formal diffeomorphisms",
abstract = "We consider a generalization of (pro)algebraic loops defined on general categories of algebras and the dual notion of a coloop bialgebra suitable to represent them as functors. We prove that the natural loop of formal diffeomorphisms with associative coefficients is proalgebraic, and we give the closed formulas of the codivisions on its coloop bialgebra. This result provides a generalization of the Lagrange inversion formula to series with non-commutative coefficients, and a loop-theoretic explanation to the existence of the non-commutative Faà di Bruno Hopf algebra."
}
@article{FRANKOWSKI20102120,
title = "New method for speciation analysis of aluminium fluoride complexes by HPLC–FAAS hyphenated technique",
journal = "Talanta",
volume = "80",
