@article{REZAEI2009787,
title = "Nano-level determination of copper with atomic absorption spectrometry after pre-concentration on N,N-(4-methyl-1,2-phenylene)diquinoline-2-carboxamide–naphthalene",
journal = "Journal of Hazardous Materials",
volume = "168",
