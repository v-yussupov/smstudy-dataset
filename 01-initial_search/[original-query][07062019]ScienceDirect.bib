journal = "Talanta",
volume = "189",
pages = "182 - 189",
year = "2018",
issn = "0039-9140",
doi = "https://doi.org/10.1016/j.talanta.2018.06.053",
url = "http://www.sciencedirect.com/science/article/pii/S0039914018306593",
author = "Pawel Pohl and Anna Dzimitrowicz and Piotr Jamroz and Krzysztof Greda",
keywords = "Apple juice, Major and minor elements, Simplified sample preparation, High resolution-continuum source-flame atomic absorption spectrometry, Response surface design, Optimization",
abstract = "A new method for fast sequential measurements of concentrations of Ca, Fe, K, Mg, Mn and Na in apple juices by high resolution-continuum source-flame atomic absorption spectrometry (HR-CS-FAAS) was developed using response surface methodology (RSM). By combination of Box-Behnken response surface design and desirability functions, compromise conditions of simplified sample preparation of apple juices, including 2-fold dilution with water and acidification with HNO3 to 0.8 mol L–1, were established and verified that provided reliable results. The method was applied for analysis of 20 different samples of commercially sold apple juices. Precision (as relative standard deviation for 3 parallel samples, RSD) and accuracy (as relative error in relation to reference values obtained with prior wet digestion of samples) were very good, i.e. from 0.5% to 6% and from –5 to + 6%, respectively."
}
@article{POHL2018248,
title = "HR-CS FAAS based method for direct determination of total concentrations of Ca, Fe, Mg and Mn in functional apple beverages and evaluation of contributions of the bioaccessible fraction of these elements by in vitro gastrointestinal digestion and chemical fractionation",
journal = "Microchemical Journal",
volume = "140",
pages = "248 - 255",
year = "2018",
issn = "0026-265X",
doi = "https://doi.org/10.1016/j.microc.2018.04.032",
url = "http://www.sciencedirect.com/science/article/pii/S0026265X18302911",
author = "Pawel Pohl and Anna Dzimitrowicz and Piotr Jamroz and Krzysztof Greda",
keywords = "Functional apple beverages, Minerals, Direct analysis, Sample preparation, Bioaccessibility, Fractionation analysis",
abstract = "Functional apple beverages, including organic apple juices and apple juices with added species, were studied on the content of total concentrations of Ca, Fe, Mg and Mn as well as bioaccessibility of these elements and their chemical fractionation patterns. A new simplified sample preparation procedure was developed and validated for determination of Ca, Fe, Mg and Mn by high resolution-continuum source flame atomic absorption spectrometry (HR-CS FAAS). Bioaccessibility of Ca, Fe, Mg and Mn from analyzed apple beverages was assessed using in vitro simulated gastrointestinal digestion while chemical partitioning of elements species was carried out with the aid of tandem-column solid phase extraction (SPE). It was established that Ca, Mg and Mn are highly bioaccessible from functional apple beverages (>94% of total concentrations), significantly contributing to recommended daily intakes (RDIs) of these elements, i.e. up to 11%, 19% and 21% per 1 L, respectively, for Ca, Mg and Mn."
}
@article{POHL2019538,
title = "Development of a very simple and fast analytical methodology for FAAS/FAES measurements of Ca, K, Mg and Na in red beetroot juices along with chemical fractionation of Ca and Mg by solid phase extraction",
journal = "Microchemical Journal",
volume = "147",
pages = "538 - 544",
year = "2019",
issn = "0026-265X",
doi = "https://doi.org/10.1016/j.microc.2019.03.075",
url = "http://www.sciencedirect.com/science/article/pii/S0026265X19304564",
author = "Pawel Pohl and Malgorzata Kalinka and Mateusz Pieprz",
keywords = "Red beetroot juice, Macroelements, Flame atomic absorption spectrometry, Sample preparation, Solid phase extraction, Chemical fractionation",
abstract = "A new simplified method of flame atomic absorption and emission spectrometry (FAAS/FAES) analysis of red beetroot juices on the content of Ca, K, Mg, and Na was proposed and validated. The method included only appropriate dilution of samples with 0.5 mol L−1 HNO3 prior to spectrometric measurements and was characterized by precision better than 2% and trueness better than 3%. It was used to verify quality of twelve commercially available red beetroot juices. In addition, chemical fractionation of Ca and Mg was undertaken with the aid of tandem solid phase extraction (SPE) with reversed phase and cation exchange sorptive materials. Three operationally defined fractions, including hydrophobic, residual, and cationic species, were distinguished. On the basis of contributions of these fractions it was presumed that Ca and Mg were organically bound to a high degree, i.e., on average 86% in case of Ca and 75% in case of Mg."
}
