@article{OZTURKER201813,
title = "Trace determination of cobalt in biological fluids based on preconcentration with a new competitive ligand using dispersive liquid-liquid microextraction combined with slotted quartz tube–flame atomic absorption spectrophotometry",
journal = "Journal of Trace Elements in Medicine and Biology",
