@article{EZODDIN2010900,
title = "Application of modified nano-alumina as a solid phase extraction sorbent for the preconcentration of Cd and Pb in water and herbal samples prior to flame atomic absorption spectrometry determination",
journal = "Journal of Hazardous Materials",
volume = "178",
