@article{DINIZ2013838,
title = "Synthesis and adsorption studies of novel hybrid mesoporous copolymer functionalized with protoporphyrin for batch and on-line solid-phase extraction of Cd2+ ions",
journal = "Reactive and Functional Polymers",
volume = "73",
number = "6",
pages = "838 - 846",
year = "2013",
issn = "1381-5148",
doi = "https://doi.org/10.1016/j.reactfunctpolym.2013.03.017",
url = "http://www.sciencedirect.com/science/article/pii/S1381514813000771",
author = "Kristiany Moreira Diniz and Mariana Gava Segatelli and César Ricardo Teixeira Tarley",
keywords = "Hybrid copolymer [poly(ethylene glycol dimethacrylate--protoporphyrin)-silica], Preconcentration, FIA-FAAS, Isotherm",
abstract = "A hybrid copolymer [poly(ethylene glycol dimethacrylate-co-protoporphyrin)-silica], synthesized by free radical copolymerization and sol–gel process was evaluated as novel adsorbent for solid-phase extraction of Cd2+ ions. Characterization of hybrid polymer was performed by FTIR, SEM and surface area analyzes. Adsorption isotherms built at pH 8.9 were very well adjusted (R2=0.9982) to hybrid non-linear Langmuir–Freundlich model for two sites, indicating the existence of different affinity constants for binding sites, which was confirmed by Scatchard plot. The estimated maximum adsorption capacity was found to be 8.28mgg−1. The adsorption kinetics data also corroborated to the isotherm, where the Cd2+ ions adsorption followed the pseudo-second-order kinetic (R2=0.998). The on-line preconcentration procedure, optimized by means of factorial designs, was based on sample preconcentration (16mL) at pH 8.9 through 50.0mg of hybrid copolymer packed in mini-column at 8.0mLmin−1 flow rate. The on-line desorption of Cd2+ ions towards the FAAS detector was carried out in countercurrent at 5.0mLmin−1 flow rate using 0.8molL−1 HNO3. Using the on-line preconcentration procedure, the maximum adsorption capacity determined from breakthrough curve was found to be 2.25mgg−1. Analytical curve ranged from 0.0 up to 50.0μgL−1 (r=0.997), limit of detection of 0.27μgL−1, preconcentration factor of 38.4, sample throughput of 30h−1 and consumptive index of 0.41mL, were achieved. The preconcentration method, very tolerable to several foreign ions, was successfully applied to the Cd2+ ions determination in water samples and cigarette sample. The accuracy was checked from analysis of certified reference materials."
}
@article{DINIZ2014233,
title = "Preparation of SiO2/Nb2O5/ZnO mixed oxide by sol–gel method and its application for adsorption studies and on-line preconcentration of cobalt ions from aqueous medium",
